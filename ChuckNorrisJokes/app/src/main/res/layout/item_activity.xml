<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                             xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
                                             android:layout_height="match_parent">

    <TextView
            android:id="@+id/tvIcon_url"
            android:text="icon_url"
            android:layout_width="67dp"
            android:layout_height="35dp"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp" android:layout_marginTop="60dp" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.501"/>
    <TextView
            android:id="@+id/tvId"
            android:text="Id"
            android:layout_width="66dp"
            android:layout_height="35dp"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp" android:layout_marginTop="60dp"
            app:layout_constraintTop_toBottomOf="@+id/tvIcon_url" app:layout_constraintHorizontal_bias="0.503"/>
    <TextView
            android:id="@+id/tvUrl"
            android:text="Url"
            android:layout_width="69dp"
            android:layout_height="34dp"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp" android:layout_marginTop="60dp"
            app:layout_constraintTop_toBottomOf="@+id/tvId" app:layout_constraintHorizontal_bias="0.494"/>
    <TextView
            android:id="@+id/tvValue"
            android:text="Value"
            android:layout_width="70dp"
            android:layout_height="33dp"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp" android:layout_marginTop="60dp"
            app:layout_constraintTop_toBottomOf="@+id/tvUrl" app:layout_constraintHorizontal_bias="0.496"/>
</android.support.constraint.ConstraintLayout>